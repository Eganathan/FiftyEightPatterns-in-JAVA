public class FiftyEightPatterns {
    /*
    *Printing the pattern...
    *5
    *5 4
    *5 4 3
    *5 4 3 2
    5 4 3 2 1
    */

    public static void main(String[] args){
        System.out.println(" Pattern 08");

        for (int x= 5; x>= 1; x--){
            for (int y = 5; y >= x; y--){
                System.out.print( y + " ");
            }
            System.out.println("");
        }





    }
}
/* 
* x = 5,4,3,2,1
* x5 = y = 5
* x4 = y = 5,4...
*
* */
